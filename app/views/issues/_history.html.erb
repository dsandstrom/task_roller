<% if history.is_a?(IssueReopening) %>
  <%= render history %>
<% elsif history.is_a?(IssueClosure) %>
  <%= render history %>
<% elsif history.is_a?(Task) %>
  <%= render 'tasks/history', history: history %>
<% elsif history.is_a?(Resolution) %>
  <%= render 'resolutions/history', history: history %>
<% elsif history.is_a?(IssueConnection) %>
  <%= render history %>
<% end %>
