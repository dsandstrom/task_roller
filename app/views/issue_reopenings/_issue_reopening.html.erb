<% value = "reopened issue: #{format_date issue_reopening.created_at}" %>

<%= content_tag :dt, issue_reopening.user&.name_or_email,
                class: 'issue-reopening-user' %>
<%= content_tag :dd, value,
                class: "issue-reopening",
                id: "issue-reopening-#{issue_reopening.id}" %>
